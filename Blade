
local Library = loadstring(Game:HttpGet("https://raw.githubusercontent.com/Hinm12/CoreAbsoluteCopy/main/wizard.txt"))()

local PhantomForcesWindow = Library:NewWindow("Blade Ball")

local AutoParry = PhantomForcesWindow:NewSection("Main")

AutoParry:CreateButton("Hold Block To Spam", function()getgenv().SpamSpeed = 1
loadstring(game:HttpGet("https://raw.githubusercontent.com/Hinm12/CoreAbsoluteCopy/main/MC_Blade%20Ball%20Spam.txt",true))()
end)

AutoParry:CreateToggle("Auto Parry", function()getgenv().config = getgenv().config or {
    hit_time = 0.5, -- // recommended 0.25 to 0.75 \ --
    
    mode = 'Always', -- // Hold , Toggle , Always \ --
    deflect_type = 'Remote', -- // Key Press , Remote \ --
    notifications = true,
    keybind = Enum.KeyCode.V
}

loadstring(game:HttpGet("https://raw.githubusercontent.com/Hinm12/CoreAbsoluteCopy/main/MC_Blade%20Ball%20Parry%20V4.0.0%20(1).txt",true))()
end)

AutoParry:CreateToggle("Auto Clash", function()loadstring(game:HttpGet("https://raw.githubusercontent.com/Hinm12/CoreAbsoluteCopy/main/t2391h1A.txt"))()
end)


 
